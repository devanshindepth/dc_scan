{
  "name": "ai-dev-insights",
  "displayName": "AI Development Insights",
  "description": "Privacy-safe insights into AI tool usage patterns and coding effectiveness",
  "version": "0.1.0",
  "publisher": "ai-dev-insights",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aiDevInsights.pauseTracking",
        "title": "Pause Tracking",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.resumeTracking",
        "title": "Resume Tracking",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.toggleTracking",
        "title": "Toggle Tracking",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.showInsights",
        "title": "Show Insights",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.exportData",
        "title": "Export Data",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.clearData",
        "title": "Clear All Data",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.showPrivacyInfo",
        "title": "Privacy Information",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.openPrivacySettings",
        "title": "Privacy Settings",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.statusBarMenu",
        "title": "Status Bar Menu",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.openSettings",
        "title": "Open Settings",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.viewLogs",
        "title": "View Logs",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.exportLogs",
        "title": "Export Logs",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.clearOldLogs",
        "title": "Clear Old Logs",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.syncLogs",
        "title": "Sync Logs to Backend",
        "category": "AI Dev Insights"
      },
      {
        "command": "aiDevInsights.showLogStats",
        "title": "Show Log Statistics",
        "category": "AI Dev Insights"
      }
    ],
    "configuration": {
      "title": "AI Development Insights",
      "properties": {
        "aiDevInsights.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable AI Development Insights tracking"
        },
        "aiDevInsights.backendUrl": {
          "type": "string",
          "default": "http://localhost:3000",
          "description": "Backend URL for syncing insights data"
        },
        "aiDevInsights.syncInterval": {
          "type": "number",
          "default": 300000,
          "description": "Sync interval in milliseconds (default: 5 minutes)"
        },
        "aiDevInsights.retentionDays": {
          "type": "number",
          "default": 30,
          "description": "Number of days to retain local data",
          "minimum": 1,
          "maximum": 365
        },
        "aiDevInsights.syncEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable syncing data to backend server"
        },
        "aiDevInsights.showPrivacyReminders": {
          "type": "boolean",
          "default": true,
          "description": "Show periodic privacy information reminders"
        },
        "aiDevInsights.allowAnonymousUsage": {
          "type": "boolean",
          "default": true,
          "description": "Allow anonymous usage analytics to help improve the extension"
        },
        "aiDevInsights.showStatusBar": {
          "type": "boolean",
          "default": true,
          "description": "Show AI Development Insights status in the status bar"
        }
      }
    },
    "keybindings": [
      {
        "command": "aiDevInsights.toggleTracking",
        "key": "ctrl+shift+alt+t",
        "mac": "cmd+shift+alt+t",
        "when": "editorTextFocus"
      },
      {
        "command": "aiDevInsights.openPrivacySettings",
        "key": "ctrl+shift+alt+p",
        "mac": "cmd+shift+alt+p"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "aiDevInsights.pauseTracking",
          "when": "config.aiDevInsights.enabled"
        },
        {
          "command": "aiDevInsights.resumeTracking",
          "when": "!config.aiDevInsights.enabled"
        },
        {
          "command": "aiDevInsights.toggleTracking"
        },
        {
          "command": "aiDevInsights.showInsights"
        },
        {
          "command": "aiDevInsights.exportData"
        },
        {
          "command": "aiDevInsights.clearData"
        },
        {
          "command": "aiDevInsights.showPrivacyInfo"
        },
        {
          "command": "aiDevInsights.openPrivacySettings"
        },
        {
          "command": "aiDevInsights.openSettings"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.0",
    "@types/node": "18.x",
    "@types/sql.js": "^1.4.9",
    "@types/vscode": "^1.74.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.1",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "node-fetch": "^3.3.0",
    "sql.js": "^1.13.0",
    "uuid": "^9.0.0"
  }
}